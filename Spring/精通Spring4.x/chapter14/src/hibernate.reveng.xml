<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="sampledb" match-name="t_forum"/>
  <table-filter match-catalog="sampledb" match-name="t_post" exclude="true"/>
  <table-filter match-catalog="sampledb" match-name="t_post_id" exclude="true"/>
  <table-filter match-catalog="sampledb" match-name="t_topic" exclude="true"/>
  <table-filter match-catalog="sampledb" match-name="t_user" exclude="true"/>
</hibernate-reverse-engineering>